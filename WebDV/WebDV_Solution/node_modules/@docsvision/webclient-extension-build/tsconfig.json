{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "ES2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "experimentalDecorators": true,
    "lib": [ "dom", "es6" ],
    "allowSyntheticDefaultImports": true,
    "keyofStringsOnly": true
  }
}